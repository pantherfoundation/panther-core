{
  "name": "@panther-core/circuits",
  "version": "1.0.0",
  "description": "Panther Protocol Circuits",
  "license": "UNLICENSED",
  "private": true,
  "repository": "git+https://github.com/pantherprotocol/panther-circuits.git",
  "scripts": {
    "build": "yarn build:types && yarn build:ts",
    "build:main-amm": "./scripts/buildAMM.sh",
    "build:main-z-transaction": "./scripts/buildMainTransactionV1Extended.sh",
    "build:main-z-account-registration-v1-extended": "./scripts/buildMainZaccountRegistrationV1Extended.sh",
    "build:main-z-account-renewal-v1-extended": "./scripts/buildMainZaccountRenewalV1Extended.sh",
    "build:main-z-swap": "./scripts/buildMainZSwap.sh",
    "build:main-tree-batch-updater-and-root-checker": "./scripts/buildMainTreeBatchUpdaterAndRootChecker.sh",
    "build:main-everything": "yarn build:main-amm && yarn build:main-z-transaction && yarn build:main-z-swap && yarn build:main-z-account-registration-v1-extended && yarn build:main-z-account-renewal-v1-extended && yarn build:main-tree-batch-updater-and-root-checker",
    "build:types": "./scripts/genTypes.sh",
    "build:ts": "tsc -p tsconfig.json",
    "clean": "rm -rf ./node_modules && rm -rf ./types/* && rm -rf ./lib",
    "compile:main-z-transaction-docker": "scripts/circomDocker.sh --r1cs --wasm --sym -o compiled/ circuits/mainZTransactionV1.circom",
    "compile:main-amm": "circom --r1cs --wasm --sym -o compiled/ circuits/mainAmmV1.circom",
    "compile:main-tree-batch-updater-and-root-checker": "circom --r1cs --wasm --sym -o compiled/ circuits/mainTreeBatchUpdaterAndRootChecker.circom",
    "compile:main-z-transaction": "circom --r1cs --wasm --sym -o compiled/ circuits/mainZTransactionV1.circom",
    "compile:main-z-swap": "circom --r1cs --wasm --sym -o compiled/ circuits/mainZSwapV1.circom",
    "compile:main-z-swap-top": "circom --r1cs --wasm --sym -o compiled/ circuits/mainZSwapV1Top.circom",
    "compile:main-z-account-registration": "circom --r1cs --wasm --sym -o compiled/ circuits/mainZAccountRegistrationV1.circom",
    "compile:main-z-account-renewal": "circom --r1cs --wasm --sym -o compiled/ circuits/mainZAccountRenewalV1.circom",
    "compile:data-escrow-elgamal-ecnryption": "circom --r1cs --wasm --sym -o compiled/ test/circuits/dataEscrowElGamalEncryption.circom",
    "compile:main-range-check": "circom --r1cs --wasm --sym -o compiled/ circuits/mainRangeCheck.circom",
    "compile:main-everything": "yarn compile:main-z-transaction && yarn compile:main-z-swap && yarn compile:main-z-account-registration && yarn compile:main-z-account-renewal && yarn compile:main-amm && yarn compile:main-tree-batch-updater-and-root-checker",
    "compute:static-merkle-root": "rm -rf lib/ && yarn build && cd lib/api && node staticTreeComputation.js",
    "inspect:main-amm": "circom --inspect --r1cs --wasm --sym -o compiled/ circuits/mainAmmV1.circom",
    "inspect:main-z-transaction": "circom --inspect --r1cs --wasm --sym -o compiled/ circuits/mainZTransactionV1.circom",
    "inspect:main-z-swap": "circom --inspect --r1cs --wasm --sym -o compiled/ circuits/mainZSwapV1.circom",
    "inspect:main-z-swap-top": "circom --inspect --r1cs --wasm --sym -o --O2 compiled/ circuits/mainZSwapV1Top.circom",
    "inspect:main-z-account-registration": "circom --inspect --r1cs --wasm --sym -o compiled/ circuits/mainZAccountRegistrationV1.circom",
    "inspect:main-z-account-renewal": "circom --inspect --r1cs --wasm --sym -o compiled/ circuits/mainZAccountRenewalV1.circom",
    "inspect:main-everything": "yarn inspect:main-z-transaction && yarn inspect:main-z-swap && yarn inspect:main-z-account-registration && yarn inspect:main-z-account-renewal && yarn inspect:main-amm",
    "circomspect:amm": "circomspect circuits/ammV1.circom",
    "circomspect:z-transaction": "circomspect circuits/zTransactionV1.circom",
    "circomspect:z-swap": "circomspect circuits/zSwapV1.circom",
    "circomspect:z-account-registration": "circomspect circuits/zAccountRegistrationV1.circom",
    "circomspect:z-account-renewal": "circomspect circuits/zAccountRenewalV1.circom",
    "circomspect:everything": "yarn circomspect:z-transaction && yarn circomspect:z-swap && yarn circomspect:z-account-registration && yarn circomspect:z-account-renewal && yarn circomspect:amm",
    "lint": "yarn eslint && yarn prettier:list-different",
    "lint:eslint": "eslint --max-warnings 0",
    "lint:circom": "yarn lint:eslint \"circuits/**/*.circom\"",
    "prettier:check": "cd .. && yarn prettier --check circuits",
    "prettier:fix": "cd .. && yarn prettier --write circuits",
    "prettier:list-different": "cd .. && yarn prettier --list-different circuits",
    "test": "yarn build && mocha 'lib/**/*.js' --max-old-space-size=4000 --timeout 60000 --exit",
    "test:long": "export RUN_LONG_TESTS=TRUE && cd test && ./run-all-tests.sh",
    "test:docker": "export CIRCOM_DOCKER=yes && mocha --timeout 15000"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "circom_tester": "^0.0.19",
    "circomlibjs": "^0.0.8",
    "mocha": "^9.1.3",
    "snarkjs": "^0.6.11",
    "@zk-kit/merkle-tree": "^0.1.0",
    "ffjavascript": "^0.2.62"
  },
  "dependencies": {
    "circomlib": "^2.0.5",
    "typescript": "^4.9.4",
    "ethers": "^6.3.0"
  },
  "main": "index.js",
  "author": "Pantherprotocol team"
}
